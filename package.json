{
  "name": "listcommunity",
  "homepage": "https://list.community",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "algoliasearch": "^3.24.8",
    "base-64": "^0.1.0",
    "contrast": "^1.0.1",
    "core-js": "^2.5.3",
    "gh-pages": "^1.1.0",
    "github-markdown-css": "^2.9.0",
    "htmr": "github:listcommunity/htmr#dist",
    "is-absolute-url": "^2.1.0",
    "lodash.chunk": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.difference": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "npm-run-all": "^4.1.2",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-scroll-up": "^1.3.3",
    "tailwindcss": "^0.4.0",
    "url-join": "^2.0.2",
    "watch": "^1.0.2"
  },
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "start:css": "watch 'npm run build:css' ./src/tailwind",
    "start:js": "react-scripts start",
    "build": "npm-run-all build:css build:js",
    "build:css": "tailwind build ./src/tailwind/index.css -c ./src/tailwind/config.js -o ./src/index.css",
    "build:js": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "jsxBracketSameLine": true
  }
}
